{
  "name": "eds",
  "version": "1.0.0",
  "description": "Ericsson Design System",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "iebuild": "ng serve --prod --port 8080 --host 0.0.0.0",
    "build": "ng build --prod",
    "build:snapshot": "ng build --base-href /snapshot/",
    "test": "ng test",
    "tests": "npm run lint --no-progress && npm run spellcheck && npm run eslint --no-progress && npm run build",
    "coverage": "ng test --code-coverage --watch=false",
    "lint": "ng lint",
    "eslint": "eslint ./src",
    "spellcheck": "cspell src/**/*.html",
    "e2e": "ng e2e",
    "bundle": "gulp",
    "publish:snapshot": "npm run bundle && npm publish dist-vanilla --tag next",
    "publish:release": "npm run bundle && npm publish dist-vanilla",
    "version:snapshot": "cd src/public/vanilla-components && standard-version --prerelease --skip.changelog --skip.tag -m \"Bumped prerelease version\"",
    "version:release": "cd src/public/vanilla-components && standard-version -m \"Bumped release version\"",
    "version:localchangelog": "cd src/public/vanilla-components && standard-version --skip.commit --skip.tag -m \"Bumped release version\""
  },
  "private": true,
  "publishConfig": {
    "registry": "https://arm.rnd.ki.sw.ericsson.se/artifactory/api/npm/proj-eds-npm-local/"
  },
  "dependencies": {
    "@angular/animations": "^7.2.0",
